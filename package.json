{
  "name": "n8n-oracle-query-node",
  "version": "1.0.0",
  "description": "Node for Oracle Database with parameterization",
  "keywords": [
    "n8n-community-node-package",
    "oracle",
    "query",
    "node",
    "n8n",
    "database"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Felipe Menezes",
    "email": "fmenezes@ccmtecnologia.com.br"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/felipemenezes98/n8n-nodes-oracle"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lintfix": "tslint --fix -p tsconfig.json -c tslint.json",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/Oracle.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Oracle/OracleDatabase.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/oracledb": "^5.3.0",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint-plugin-n8n-nodes-base": "^1.5.4",
    "gulp": "^4.0.2",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0",
    "prettier": "^2.7.1",
    "tslint": "^6.1.2",
    "typescript": "~4.6.0"
  },
  "dependencies": {
    "oracledb": "^6.0.1"
  }
}
